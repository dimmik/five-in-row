
<circle cx=@x cy=@y r="@(size / 2)" stroke="#00008B" fill="none" stroke-linecap="round" stroke-width="4" stroke-opacity=@opacity />

@code {
    [Parameter]
    public int x { get; set; } = 0;
    [Parameter]
    public int y { get; set; } = 0;
    [Parameter]
    public int size { get; set; } = 10;
    [Parameter]
    public string opacity { get; set; } = "1";
}
